@using PiralSample.Components.Layout
<Piral.Blazor.Orchestrator.MfRouter AppAssembly="@typeof(App).Assembly">
    <Found Context="routeData">
        <Piral.Blazor.Orchestrator.MfRouteView RouteData="@routeData" DefaultLayout="@typeof(MainLayout)" />
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
    </Found>
    <NotFound>
        <PageTitle>Not found</PageTitle>
        <LayoutView Layout="@typeof(MainLayout)">
            <p role="alert">Sorry, there's nothing at this address.</p>
        </LayoutView>
    </NotFound>
</Piral.Blazor.Orchestrator.MfRouter>